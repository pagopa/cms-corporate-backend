{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "url_path": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "unique": false,
      "minLength": 5,
      "regex": "^\\/(?!.*\\/\\/)([a-zA-Z-\\/]+)$"
    },
    "parent": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::page.page"
    },
    "blocks": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "shared.block-accordion",
        "shared.block-attachments-grid",
        "shared.block-contacts-list",
        "shared.block-contents-list",
        "shared.block-cta-banner",
        "shared.block-cta-grid",
        "shared.block-hero-slider",
        "shared.block-intro",
        "shared.block-jobs-listing",
        "shared.block-list-attachments",
        "shared.block-logo-links",
        "shared.block-map-box",
        "shared.block-media-highlights",
        "shared.block-news-and-events",
        "shared.block-press-release",
        "shared.block-projects-carousel",
        "shared.block-text-video-option",
        "shared.block-university-accordion",
        "shared.block-visual-text",
        "shared.block-visual",
        "shared.block-newsletter",
        "shared.block-innovation-announcements"
      ]
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.seo"
    },
    "bannerNewsletter": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "boolean",
      "default": false
    }
  }
}
